{
  "multipart": [
    {   "apply": { "model": "militarybasedecor:chain_link_fence_post" }},
    {   "when": { "north": true },
      "apply": { "model": "militarybasedecor:chain_link_fence_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "militarybasedecor:chain_link_fence_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "militarybasedecor:chain_link_fence_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "militarybasedecor:chain_link_fence_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "militarybasedecor:chain_link_fence_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "militarybasedecor:chain_link_fence_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "militarybasedecor:chain_link_fence_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "militarybasedecor:chain_link_fence_noside", "y": 270 }
    }
  ]
}